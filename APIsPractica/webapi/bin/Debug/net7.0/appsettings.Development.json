{
  "Logging": {
    "LogLevel": { // Cual es el nivel de error que se va a mostrar dentro de la consola al momento en que la aplicacion se este ejecutando
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
// LogError nos permite capturar que tipo de error, indicar cual es el nivel de error que quiero capturar: trace = 0, debug = 1, information = 2, warning = 3, error = 4, critical = 5, none = 6
// Si LogLevel es information trace y debug no se mostrarian, si decimos que es error solo error y critical se mostrarian
// Normalmente de Information y Error, y cuando estamos trabajando local en modo desarrolaldor se utiliza debug
